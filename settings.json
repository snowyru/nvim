{
    "workbench.colorTheme": "Activate UMBRA protocol",
    "workbench.sideBar.location": "right",
    "git.enableSmartCommit": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "tabnine.experimentalAutoImports": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "prettier.enable": false,
    "prettier.prettierPath": ".prettiermore",
    "prettier.requireConfig": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.accessibilitySupport": "off",
    "C_Cpp.default.compilerPath": "C:\\msys64\\mingw64\\bin\\gcc.exe",
    "code-runner.runInTerminal": true,
    "[python]": {
        "editor.formatOnType": true
    },
    "cmake.additionalCompilerSearchDirs": [
        "C:/msys64/mingw32/bin",
        "C:/msys64/clang32/bin",
        "C:/msys64/clang64/bin",
        "C:/msys64/clangarm64/bin",
        "C:/msys64/ucrt64/bin"
    ],
    "cmake.configureOnOpen": true,
    "C_Cpp_Runner.cCompilerPath": "C:/msys64/mingw64/bin/gcc.exe",
    "code-runner.languageIdToFileExtensionMap": {
        "bat": ".bat",
        "powershell": ".ps1",
        "typescript": ".ts"
    },
    "code-runner.executorMapByGlob": {

        "pom.xml": "cd $dir && mvn clean package"
    },
    "code-runner.executorMap": {
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt -lncurses -DNCURSES_STATIC && ./$fileNameWithoutExt"
    },
    "git.openRepositoryInParentFolders": "never",
    "redhat.telemetry.enabled": false,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "window.zoomLevel": 1,
    "java.completion.filteredTypes": [
        "java.awt.*",
        "com.sun.*",
        "sun.*",
        "jdk.*",
        "org.graalvm.*",
        "io.micrometer.shaded.*"
    ],
    "editor.lineNumbers": "relative"
}